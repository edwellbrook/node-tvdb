{
    "plugins": [
        "mocha"
    ],
    "rules": {
        "mocha/no-exclusive-tests": 2,
        "mocha/handle-done-callback": 2,
        "mocha/no-global-tests": 2,
        "mocha/no-pending-tests": 2,
        "mocha/no-skipped-tests": 1,
        "mocha/no-synchronous-tests": 2,
        "semi": [
            2,
            "always"
        ],
        "no-bitwise": 2,
        "curly": 2,
        "eqeqeq": [
            2,
            "smart"
        ],
        "guard-for-in": 2,
        "no-native-reassign": 2,
        "no-caller": 2,
        "no-empty": 2,
        "no-undef": 2,
        "no-unused-vars": [
            2,
            {
                "vars": "all",
                "args": "after-used"
            }
        ],
        "strict": [2, "global"],
        "new-cap": 2,
        "block-scoped-var": 2,
        "comma-dangle": [
            2,
            "never"
        ],
        "no-cond-assign": [
            2,
            "always"
        ],
        "no-constant-condition": 2,
        "no-dupe-args": 2,
        "no-dupe-keys": 2,
        "no-duplicate-case": 2,
        "no-empty-character-class": 2,
        "no-ex-assign": 2,
        "no-extra-boolean-cast": 2,
        "no-extra-semi": 2,
        "no-func-assign": 2,
        "no-inner-declarations": 2,
        "no-invalid-regexp": 2,
        "no-irregular-whitespace": 2,
        "no-obj-calls": 2,
        "no-sparse-arrays": 2,
        "no-unexpected-multiline": 2,
        "no-unreachable": 2,
        "use-isnan": 2,
        "valid-typeof": 2,
        "consistent-return": 2,
        "dot-notation": 2,
        "dot-location": [2, "property"],
        "no-labels": 2,
        "no-empty-pattern": 2,
        "no-eval": 2,
        "no-extra-bind": 2,
        "no-fallthrough": 2,
        "no-floating-decimal": 2,
        "no-implicit-coercion": [
            2,
            {
                "boolean": false,
                "number": true,
                "string": true
            }
        ],
        "no-implied-eval": 2,
        "no-iterator": 2,
        "no-lone-blocks": 2,
        "no-loop-func": 2,
        "no-multi-str": 2,
        "no-new-func": 2,
        "no-new-wrappers": 2,
        "no-octal-escape": 2,
        "no-octal": 2,
        "no-proto": 2,
        "no-redeclare": 2,
        "no-script-url": 2,
        "no-self-compare": 2,
        "no-sequences": 2,
        "no-trailing-spaces": 2,
        "no-useless-call": 2,
        "no-useless-concat": 2,
        "no-void": 2,
        "no-with": 2,
        "radix": 2,
        "no-catch-shadow": 2,
        "no-delete-var": 2,
        "no-shadow-restricted-names": 2,
        "no-undef-init": 2,
        "new-parens": 2,
        "no-array-constructor": 2,
        "no-mixed-spaces-and-tabs": 2,
        "no-nested-ternary": 2,
        "no-new-object": 2,
        "no-unneeded-ternary": 2,
        "indent": [
            2,
            4,
            {"SwitchCase": 1}
        ],
        "no-multiple-empty-lines": [
            2,
            {
                "max": 1,
                "maxEOF": 10
            }
        ],
        "no-extend-native": 2,
        "no-throw-literal": 2,
        "brace-style": [
            2,
            "stroustrup",
            {
                "allowSingleLine": true
            }
        ],
        "camelcase": 2,
        "func-style": [
            2,
            "declaration"
        ],
        "no-alert": 2,
        "no-case-declarations": 2,
        "no-else-return": 2,
        "no-implicit-globals": 2,
        "no-invalid-this": 2,
        "callback-return": 2,
        "no-label-var": 2,
        "no-shadow": 2,
        "no-mixed-requires": 2,
        "array-bracket-spacing": [2, "never"],
        "block-spacing": [2, "never"],
        "comma-spacing": [2, {"before": false, "after": true}],
        "comma-style": [2, "last"],
        "no-console": 2,
        "no-var": 2
    },
    "globals": {
        "_": false,
        "inject": false,
        "before": false,
        "after": false
    },
    "env": {
        "node": true,
        "es6": true,
        "mocha": true
    }
}
