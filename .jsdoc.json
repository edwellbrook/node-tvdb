{
  "source": {
    "include": [
      "dist/index.js",
      "README.md"
    ]
  },
  "templates": {
    "default": {
      "outputSourceFiles": false,
      "useLongnameInNav": true
    }
  },
  "plugins": [
    "plugins/markdown"
  ],
  "markdown": {
    "tags": [
      "description"
    ]
  },
  "opts": {
    "destination": "./docs",
    "encoding": "utf8",
    "template": "./node_modules/@edwellbrook/minami"
  }
}